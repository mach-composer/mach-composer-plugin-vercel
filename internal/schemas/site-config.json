{
  "type": "object",
  "description": "Global Vercel configuration",
  "properties": {
    "api_token": {
      "type": "string"
    },
    "team_id": {
      "type": "string"
    },
    "project_config": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "framework": {
          "type": "string"
        },
        "manual_production_deployment": {
          "type": "boolean"
        },
        "serverless_function_region": {
          "type": "string"
        },
        "environment_variables": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              },
              "value": {
                "type": "string"
              },
              "environment": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        },
        "git_repository": {
          "type": "object",
          "properties": {
            "production_branch": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "repo": {
              "type": "string"
            }
          }
        },
        "build_command": {
          "type": "string"
        },
        "root_directory": {
          "type": "string"
        },
        "node_version": {
          "type": "string"
        },
        "domains": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "domain": {
                "type": "string"
              },
              "git_branch": {
                "type": "string"
              },
              "redirect": {
                "type": "string"
              },
              "redirect_status_code": {
                "type": "number",
                "oneOf": [
                  {
                    "enum": [301, 302, 307, 308]
                  }
                ]
              }
            }
          }
        },
        "protection_bypass_for_automation": {
          "type": "boolean"
        },
        "vercel_authentication": {
          "type": "object",
          "properties": {
            "deployment_type": {
              "enum": ["standard_protection", "all_deployments", "only_production_deployments", "only_preview_deployments"]
            }
          }
        },
        "password_protection": {
          "type": "object",
          "properties": {
            "password": {
              "type": "string"
            },
            "deployment_type": {
              "enum": ["standard_protection", "all_deployments", "only_production_deployments", "only_preview_deployments"]
            }
          }
        }
      }
    }
  }
}
